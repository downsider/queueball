{
  "name": "lexide/queueball",
  "description": "Lexide QueueBall, a message queue abstraction module",
  "license": "MIT",
  "homepage": "https://github.com/lexide/queueball",
  "require": {
    "php": ">=5.4.0"
  },
  "require-dev": {
    "phpunit/phpunit": "4.*",
    "mockery/mockery": "0.9.*"
  },
  "autoload": {
    "psr-4": {
      "Lexide\\QueueBall\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Lexide\\QueueBall\\Test\\": "test"
    }
  },
  "extra": {
    "lexide/puzzle-di": {
      "files": {
        "lexide/syringe": {
          "path": "config/syringe.yml"
        }
      }
    }
  }
}
