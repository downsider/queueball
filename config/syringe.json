{
  "parameters": {
    "sqsRegion": "",
    "messageFactoryClass": "Silktide\\QueueBall\\Message\\SqsMessageFactory",
    "queueClass": "Silktide\\QueueBall\\Queue\\SqsQueue"
  },

  "services": {
    "sqsClient": {
      "class": "Aws\\Sqs\\SqsClient",
      "arguments": [
        {
          "version": "2012-11-05",
          "region": "%sqsRegion%"
        }
      ]
    },

    "messageFactory": {
      "class": "%messageFactoryClass%"
    },

    "queue": {
      "class": "%queueClass%",
      "arguments": [
        "@sqsClient",
        "@messageFactory"
      ]
    }
  }
}